/*접미사*/
/*suffix*/
.suffix-wrapper {position: relative}
.suffix-wrapper::after {position:absolute;top:7px;right:.5em;transition:all .05s ease-in-out;font-size:14px;color:gray;}

.suffix-wrapper.meter::after {content:'m';}
.suffix-wrapper.meter input {padding-right:20px;}

.suffix-wrapper.second::after {content:'초';}
.suffix-wrapper.second input {padding-right:20px;}

.suffix-wrapper.minute::after {content:'분';}
.suffix-wrapper.minute input {padding-right:20px;}

.suffix-wrapper.day::after {content:'일';}
.suffix-wrapper.day input {padding-right:20px;}

.suffix-wrapper.unit::after {content:'개';}
.suffix-wrapper.unit input {padding-right:20px;}

.suffix-wrapper.byte::after {content:'bytes';}
.suffix-wrapper.byte input {padding-right:47px;}

.suffix-wrapper.height::after {content:'Cm';}
.suffix-wrapper.height input {padding-right:47px;}

.suffix-wrapper.bdwgh::after {content:'Kg';}
.suffix-wrapper.bdwgh input {padding-right:47px;}

.suffix-wrapper.millimeter::after {content:'mm';}
.suffix-wrapper.millimeter input {padding-right:47px;}

