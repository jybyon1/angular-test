.box_com.term_wrap.center{ text-align: center}


body , body .full_height {height:100%;/*min-height:900px;*/} /*최소높이값 삭제 */

.full_height .body{height:calc(100% - 110px);}
.full_height .body .wrapper {height:calc(100%);background:#192438}   /*2020-05-20 배경색 수정*/
.full_height .body .wrapper .content_wrap {height:calc(100%);}
.full_height .body .wrapper .content_wrap .content_outbox {height:100%}

.full_height .content_outbox .tab_wrap {height:100%;box-sizing:border-box;}
.full_height .content_outbox .tab_wrap .tab_content {position:relative;height:calc(100% - 40px);box-sizing:border-box;}
.full_height .content_outbox .tab_wrap .tab_content .content_body , .content_outbox .content_body {position:relative;width:100%;height:calc(100%);} /* 20180213 수정 */
.full_height .content_outbox .tab_wrap .tab_content .content_body .content_inner, .content_outbox .content_inner {overflow-y:auto;padding-bottom:10px;}

.full_height .content_outbox .tab_wrap .tab_content .content_body .content_inner .box_com:first-child {margin-top:0;}
.full_height .content_outbox .tab_wrap .tab_content .content_bottom , .content_outbox .content_bottom {position:absolute;bottom:0;left:0;right:0;} /* 20180213 수정 */

/* 검색결과 없을때 설정 */
.full_height .content_outbox .tab_wrap .no_results.tab_content .content_inner {height:100%;padding-bottom:20px;}
.full_height .content_outbox .tab_wrap .no_results.tab_content .content_body .box_com + .tbl_col  {height:calc(100% - 45px);}
.full_height .content_outbox .tab_wrap .no_results.tab_content .content_body .tbl_col  {height:100%;} /* 20180131 추가 */

.full_height .content_outbox .tab_wrap .no_results.tab_content .content_bottom {display:none;}

/* 페이징 없을때 * bottom-padding 달라짐 */
.full_height .content_outbox .tab_wrap .no_paging.tab_content .content_inner {padding-bottom:20px;}
.full_height .content_outbox .tab_wrap .no_paging.tab_content .content_bottom {display:none;}



