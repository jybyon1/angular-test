table .input_half_box .ui_input{width:153px;}

.input_half_box .selectric-wrapper + .selectric-wrapper,
.input_half_box .date_box + .selectric-wrapper{margin-left:5px;}
/* E :20180213 수정 */

/* Checkbox/Radio + Input */
.ip_list:after{content:'';display:block;clear:both;}
.ip_list li{float:left;padding:0 12px 0 0;}
.ip_list li:last-child{padding-right:0;} /* 20180126 추가 */
.ip_list li .input_ico_box {display:inline-block;padding:3px 0 5px;}
.ip_list li .input_ico_box + .ui_input,
.ip_list li .input_ico_box + .tbtn_pos,
