/* text */
button + .ui_input, a + .ui_input, .ui_input + button, .ui_input + a,
.ui_sel +.ui_input, .selectric-wrapper + .ui_input{margin-left:2px;} /* 20180213 수정 */
.ui_input{display:inline-block;width:150px;border:1px solid #A4A4A4;height:36px;padding:0 9px;font-size:14px;color:#ddd;box-sizing:border-box;vertical-align:top;background-color:#212529}
.ui_input:read-only {color:#cecece;background-color:#212529;border-color:#A4A4A4;opacity:0.5;}
.ui_input:active, .ui_input:focus{border-color:#3970c3;}
.ui_input:disabled:active, .ui_input:disabled:focus{border-color:#f3f3f3;}
.ui_input.full{width:100%;}

input.ui_input.disabled, .ui_input:disabled, .ui_input:read-only  {background-color:#2e323d;color:#cecece;border-color:#A4A4A4;opacity:0.5;}
input.ui_input.disabled:placeholder{color:rgba(255,255,255,0.8)}
input:placeholder{color:#888}

/*error*/
.inputError {border-color:#ee2d41 !important;opacity:1 !important;}
.inputError .selectric-sel_black .selectric {border-color:#ee2d41 !important;}

/*password + hint*/
.input_group.password {position:relative;}
.input_group.password .pw_hint_btn {position:absolute;right:10px;display:inline-block;top:5px;}
.input_group.password .hint{position:absolute;left:10px;top:7px;background:#212529;display:none;width:calc(100% - 40px);white-space:normal;word-break:break-all;}
.input_group.password .hint.on{display:block;}
.input_group.password .ui_input {padding-right:40px;}


/* select */
/* select selectric 변경으로 인해 삭제 예정 20180213 */
.ui_sel {display:inline-block;vertical-align:top;-webkit-appearance:none;/* 네이티브 외형 감추기 */ -moz-appearance:none;appearance:none;width:150px;height:28px;border:1px solid #dadada;font-size:13px;padding:0 25px 0 9px;/* 여백으로 높이 설정 */ font-family:inherit;/* 폰트 상속 */ background:url(../images/ui/ui_sel_arr.png) no-repeat right 10px;/* 네이티브 화살표 대체 */ border-radius:0px;/* iOS 둥근모서리 제거 */  color:#444;} /* 20180117 수정 */
.ui_sel::-ms-expand {display:none;}
.ui_sel:disabled{background:url(../images/ui/ui_sel_arr_disabled.png) no-repeat right 8px #f7f7f7;border-color:#f7f7f7;color:#cecece;}
.ui_sel:focus, .ui_sel:active{border-color:#3970c3;}
.ui_sel option:checked{color:#3970c3;}
.ui_sel option:disabled{color:#cecece}
table .ui_sel{background-position:right 8px;}

/* radio */
.input_ico_box input[type="radio"] {position:absolute;width:1px;height:1px;padding:0;margin:-1px;overflow:hidden;clip:rect(0,0,0,0);border:0;}
.input_ico_box input[type="radio"] + label {display:inline-block;position:relative;padding-left:20px;margin-right:10px;cursor:pointer;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;color:#ddd;}
.input_ico_box input[type="radio"] + label:before {content:'';position:absolute;left:0;top:3px;width:14px;height:14px;text-align:center;background:#2e323d;border:1px solid #474a53;border-radius:100%;box-sizing:border-box;}
.input_ico_box input[type="radio"]:checked + label:before {background:#2e323d;border-color:#474a53;}
.input_ico_box input[type="radio"]:checked + label:after {content:'';position:absolute;top:7px;left:4px;width:6px;height:6px;background:#93bde9;border-radius:100%;}
.input_ico_box input[type="radio"] + label:before{top:2px;}
.input_ico_box input[type="radio"]:checked + label:after{top:6px;}


/* checkbox */
.input_ico_box input[type="checkbox"] {/* 실제 체크박스는 화면에서 숨김 */ position:absolute;width:1px;height:1px;padding:0;margin:-1px;overflow:hidden;clip:rect(0,0,0,0);border:0}
.input_ico_box input[type="checkbox"] + label {display:inline-block;position:relative;cursor:pointer;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;color:#fff;}
.input_ico_box input[type="checkbox"] + label:before {/* 가짜 체크박스 */ content:' ';display:inline-block;width:14px;/* 체크박스의 너비를 지정 */ height:14px;/* 체크박스의 높이를 지정 */ line-height:12px;/* 세로정렬을 위해 높이값과 일치 */ margin:-2px 6px 0 0;text-align:center;vertical-align:middle;background-color:#2a2d38;border:1px solid #A4A4A4;box-sizing:border-box;}  /*2020-05-21 border color 수정*/
.input_ico_box input[type="checkbox"]:checked + label:before {/* 체크박스를 체크했을때 */ content:'';display:inline-block;width:14px;height:14px;background:url(../images/ui/ico_check.png) 0 0 no-repeat #2e323d;}
.input_ico_box input[type="checkbox"]:disabled + .slider {cursor:default}

.input_ico_box input:disabled + label:before{background-color:#2a2d38 !important;border-color:#2a2d38 !important;cursor:default;} /*2020-05-21 수정*/
.input_ico_box input:disabled + label:after, .input_ico_box input:disabled + label{cursor:default;}
.input_ico_box input:checked:disabled + label:before{background-color:#2a2d38 !important;border-color:#dbdbdb !important;opacity:0.3;}
.input_ico_box input:checked:disabled + label:after{opacity:0.3}
/*.setting-group .input_ico_box input[type="checkbox"]:checked + label {color:#fff;}*/

/* textarea */
.tbl_row td.input .ui_textarea, .tbl_col td.input .ui_textarea{display:block;}
.ui_textarea{width:100%;height:57px;padding:5px;margin:0;font-size:14px;border:1px solid #1b1e29;box-sizing:border-box;color:#ddd;background-color:#192438;}
.ui_textarea:active, .ui_textarea:focus{border-color:#3970c3;}
.ui_textarea:disabled, .ui_textarea:read-only {color:#cecece !important;background-color:#212529 !important;border-color:#A4A4A4 !important;opacity:0.5 !important;}
.ui_textarea.h_large{height:156px;} /* 20180205 추가 */

/* file */
.file_wrap {display:inline-block;font-size:0;}
.file_wrap input[type="file"] {position:absolute;width:1px;height:1px;padding:0;margin:-1px;overflow:hidden;clip:rect(0,0,0,0);border:0;}
.file_wrap .file_ip_name {display:inline-block;width:239px;height:30px;padding:0 5px;/* label의 패딩값과 일치 */ font-size:13px;font-family:inherit;line-height:normal;    background-color:#383a47;    border:1px solid #1b1e29;-webkit-appearance:none;/* 네이티브 외형 감추기 */ -moz-appearance:none;appearance:none;vertical-align:top;color:#fff;}
.file_wrap {font-size:0;height:38px;vertical-align:top;padding:5px 0;display:block} /* 2020-05-22 윤은미 수정 */
.file_wrap .file_ip_name {width:238px;}
.file_wrap .file_ip_name + .tbtn_pos,.file_wrap .file_ip_name + .tbtn_dg {min-width:68px; margin-left:5px;}
.file_wrap + .file_group{display:block;margin:7px 0 5px;}
.file_group {display:block;margin:7px 0 5px;}
.file_group .file_item:last-child {margin-right:0;}
.file_group .file_item .title{display:inline-block;font-size:13px;color:#fff;text-decoration:none;}
.file_group .file_item .title span:hover{border-bottom:1px solid #fff;}
.file_group .file_item .ibtn_del {margin:3px 0 0 4px;}
.file_group .file_item {display:inline-block;min-height:19px;vertical-align:top; margin-right:12px;}


/* calender */
.ui_cal {width:161px;padding:0 9px;font-size:13px;line-height:15px;height:28px;border:1px solid #1b1e29;color:#ddd;background-color:#383a47}
.ui_cal:focus, .ui_cal:active{border-color:#3970c3;}
.ui_cal:disabled{background-color:#f3f3f3;border-color:#f3f3f3;color:#cecece;}
.ui_cal:disabled:active, .ui_cal:disabled:focus{border-color:#f3f3f3;}
.date_box {display:inline-block;position:relative;}
.date_box + .tt {margin:3px 3px 0 2px;color:#fff}

/* time */
.ui_input.clock::-webkit-calendar-picker-indicator {filter: invert(1);}
.ui_input.clock::-webkit-calendar-picker-indicator{ border-inline:  none; border:none;}
/* search */
.input_search_box{position:relative;display:inline-block;vertical-align:top;}
.input_search_box .btn_search{position:absolute;right:1px;top:1px;}
.input_search_box .btn_search{display:block;width:31px;height:26px;background:url(../images/icon/ico_search.png) 0 0 no-repeat #383a47;}
.input_search_box .ui_input:disabled + .btn_search,
.input_search_box .ui_input.disabled + .btn_search{background-position:-31px 0;background-color:#2e323d;}

/* Input + Btn */
.input_btn_box {display:inline-block;min-width:311px;min-height:24px;vertical-align:top;} /* 20180131 수정 */
.input_btn_box .tbtn_pos {min-width:68px;}
.input_btn_box .ui_input {width:240px;}


/* input half size 2개 */
.input_half_box {display:inline-block;min-width:311px;height:24px;font-size:0;vertical-align:top;}
.input_half_box .ui_sel,
.input_half_box .selectric-wrapper,
