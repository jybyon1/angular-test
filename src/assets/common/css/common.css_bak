/*스크롤*/
html {
	scrollbar-arrow-color: #404248;
	scrollbar-base-color: #434752;
	scrollbar-Face-Color: #3e404a;
	scrollbar-Track-Color: #5b5d64;;
	scrollbar-DarkShadow-Color: #3e404a;
	scrollbar-Highlight-Color: #3e404a;
	scrollbar-Shadow-Color: #3e404a;
}

::-webkit-scrollbar { border: 1px solid #1f2024;}
::-webkit-scrollbar-button:start:decrement,
::-webkit-scrollbar-button:end:increment { display: block; width: 20px;height: 20px; background: #1f2024;}
::-webkit-scrollbar-track { background: #5a5d64; border:solid 1px #1c1e2a;}
::-webkit-scrollbar-thumb:horizontal { height: 50px; width: 50px; background: #434752; border-top:solid 1px #1c1e2a; border-bottom:solid 1px #1c1e2a;}
::-webkit-scrollbar-thumb:vertical { height: 50px; width: 50px; background: #434752; border-left:solid 1px #1c1e2a; border-right:solid 1px #1c1e2a;}
/*flechas*/
::-webkit-scrollbar-button:vertical:decrement{ background-image: url(../images/ui/scroll_arrow_up.png); background-repeat: no-repeat;   background-position: 5px 7px, 0 0;}
::-webkit-scrollbar-button:vertical:increment{ background-image: url(../images/ui/scroll_arrow_down.png); background-repeat: no-repeat;   background-position: 5px 7px, 0 0;}
::-webkit-scrollbar-button:horizontal:decrement{ background-image: url(../images/ui/scroll_arrow_left.png); background-repeat: no-repeat;   background-position: 7px 5px, 0 0;}
::-webkit-scrollbar-button:horizontal:increment{ background-image: url(../images/ui/scroll_arrow_right.png); background-repeat: no-repeat;   background-position: 7px 5px, 0 0;}
::-webkit-scrollbar-corner { background:#434752;}



.wrapper {width:100%;height:100%;min-width:1280px;padding:10px 10px 10px 10px;box-sizing:border-box;}
.wrapper:after {visibility:hidden;display:block;content:"";clear:both;height:0;}
.content_wrap {position:relative;}
.content_wrap:after {visibility:hidden;display:block;content:"";clear:both;height:0;}
.sc_wrap {overflow:hidden;position:relative;padding:10px;height:624px;}
/* color */
.tc_red{color:#ea002c;}
.tc_gray{color:#444 !important;} /* 20180220 수정 */
.tc_blue{color:#3970c3;} /* 20180207 추가 */

.content_outbox {position:relative;background-color:#192438;box-sizing:border-box;}
.content_outbox.m_row_group {border-top:none;padding:10px;}
.content_outbox.m_row_group .tab_wrap .tab_content .content_inner {padding:20px;}
.content_outbox.m_row_group .content_bottom {padding:0}
.content_inner {position:relative;padding:10px 10px;box-sizing:border-box;padding-bottom:40px;height:100%;overflow-y :auto}
.content_inner .table_wrap {height:calc(100% - 62px); background-image: url("../images/common/table_header_bg.png"); background-repeat: repeat-x}
.content_inner>.content{padding:20px;   height: calc(100% - 132px);}
.content_inner>.left-group{ background:#131d29;width:435px; margin-right:10px;float: left;}
.content_inner>.right-group{width: calc(100% - 530px);float: left;vertical-align: top;background:#131d29;}

.content_bottom{position:relative;padding:0 10px 0 10px;text-align:center}
.content_bottom .bottom_inner{padding:7px 0;position:relative;background-color:#192438}
.content_bottom .bottom_inner .selectric-wrapper{ position:absolute;left:0;width:150px;}
.content_bottom .bottom_inner .btn_box_left {position: absolute; left:0; top:0;}
.content_bottom .bottom_inner .gap{display:inline-block;width:1px;height:20px;margin:4px 7px;background-color:#484b54;vertical-align:top;}
.content_bottom .bottom_inner .total_val{position:absolute;top:10px;right:0px;color:#3970c3;font-size:13px;}
.content_bottom span input {width:35px; height:28px; padding-left:10px;background-color:#383a47}
.content_bottom .fl{float: left;}
.content_bottom .fr{float: right;}


.box_both{height:39px;border-bottom:1px solid #0d101f;}
.box_both:after{content:'';display:block;clear:both;}
.box_both .fl{float:left;}
.box_both .fr{float:right;margin:5px 20px 0 0;text-align:right;}
.box_both .btn_group a + a,  .btn_group button + button,
.box_both .btn_group a + button,  .box_both .btn_group button + a{margin-left:2px;}

.box_com {margin:9px 0;height:36px;}/*overflow:hidden 시키지 말것 - selectric 서브메뉴 사라짐 */
.box_com.lower{margin-left:10px} /* 2020-05-21 추가*/
.box_com:after{content:'';display:block;clear:both;}
.box_com .ctitle{display:inline-block;color:#fff;font-size:16px;height:22px;padding-top:8px} /* 2020-05-21:font-size:15px;*/
.box_com .fl{float:left;}
.box_com .fl a {margin-left:8px}
.box_com .fl button {margin-left:8px}
.box_com .fr{position:relative;float:right;width:30%;text-align:right;}
.box_com .fr .desc{position:absolute;right:0;top:0;font-size:13px;margin-top:5px;color:#2a2f40;}
.box_com .fr .btn_group{margin-top:0px;} /*2018.04.20 변경*/


.box_com.term_wrap{margin-bottom:25px;}
.box_com.term_wrap .fr {width:50%;} /* 20180131 수정 */
.box_com.term_wrap .input_search_box{width:400px;}
.box_com.term_wrap .ui_sel,
.box_com.term_wrap .selectric-wrapper{width:150px;} /* 20180213 수정 */
.box_com.term_wrap .ui_sel + .input_search_box,
.box_com.term_wrap .selectric-wrapper + .input_search_box{margin-left:2px;} /* 20180213 수정 */
.box_com.term_wrap .input_search_box .ui_input{width:100%;}
