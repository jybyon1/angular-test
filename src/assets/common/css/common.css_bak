.header {position:relative;width:100%;min-width:1280px;height:65px;background-color:#1f222e;z-index:1;}
.header .wrapper {padding:0;height:65px;background-color:#0d101f;}
.header .logo {position:absolute;top:0;left:0;z-index:3;}
.header .logo a {display:block;padding:10px 10px;}
.header .util_menu {position:absolute;top:10px;right:0;}
.header .util_menu span {float:left; display:inline-block; padding:0 10px; position: relative}
.header .util_menu span.util_item.notification,
.header .util_menu span.util_item.inbox,
.header .util_menu span.util_item.setting { font-size:30px}
.header .util_menu span.util_item.user_name {width: 110px; font-size: 16px; line-height: 20px; }
.header .util_menu span.util_item.notification a:hover,
.header .util_menu span.util_item.inbox a:hover,
.header .util_menu span.util_item.setting a:hover,
.header .util_menu span.util_item.user_name a:hover {color:#fff}
.header .util_menu span.util_item:first-child { border-left:0; }
.header .util_menu span.util_item a {color:#ddd;}
.header .util_menu span.util_item a.util_link {text-align: right;}
.header .util_menu span.util_item .badge {color:#fff;position: absolute; right: -5px;background: #ee2d41;padding: 2px;width: 25px;border-radius: 100px;display: block;height: 25px;text-align: center;line-height: 22px;font-size:10px}

.header .util_menu .icon{padding:10px 15px; background:#3970c3; border-radius:50%;color:#fff; margin-right: 10px;}

.header .util_menu a i{font-size:25px; color:#bcbdc1; margin-top:10px;}
.header .util_item_sub{display:none;position:absolute;z-index:999; top:55px;right:0;width:auto; min-width:140px;padding:8px 27px 12px 27px; background-color:#282a36; border:1px solid #3970c3;
	box-sizing:border-box;cursor:pointer}
.util_item_sub.active {display:inline-block}

/*.header .gnb*/
.header .gnb_box{height:100%;width:calc(100% - 300px);overflow-y:hidden;display:inline-block;position:relative;padding-left:80px;vertical-align:middle;  }
.header .gnb_box .left_btn,.header .gnb_box .right_btn {width:50px;text-align:center;margin-top:19px;color:#ddd;opacity:1;cursor:pointer;border:none;background:transparent;}
.header .gnb_box .right_btn {margin-left:0 !important;}
.header .gnb_box .left_btn:disabled,.header .gnb_box .right_btn:disabled{opacity:.4 !important;cursor:default;}

.header ul.gnb {height:auto;width:100%;}
.header ul.gnb.overflow {width:calc(100% - 100px);}
.header ul.gnb > li {height:100%;padding:0 19px;display:inline-block;text-align:left;}
.header ul.gnb > li > a.gnb_link {display:inline-block;height:44px;padding-top:18px;color:#bcbdc1;font-size:19px;} /*2020-05-21 :font-size:17px 수정*/


/* active */
.header ul.gnb > li.active > a.gnb_link {height:100%;border-bottom:solid 3px #3970c3;color:#3970c3;font-weight:bold}
.header ul.gnb > li.active > a.gnb_link:hover {height:100%;border-bottom:solid 3px #3970c3;color:#3970c3;}

.header ul.gnb > li .gnb_sub {display:none;z-index:5;position:absolute;top:50px;width:auto;padding:8px 27px 12px 27px;background-color:#282a36;border:1px solid #6d2595;box-sizing:border-box;}
.header ul.gnb > li .gnb_sub li{padding:9px 0;}
.header ul.gnb > li .gnb_sub li a{color:#bcbdc1;font-size:15px;}
.header ul.gnb > li .gnb_sub li a:hover{color:#6d2595;} /* 20180205 수정 */
.header ul.gnb > li .gnb_sub li a:active{color:#6d2595;}
.header ul.gnb > li .gnb_sub li a.disabled{color:#63646b}
.header ul.gnb > li:hover .gnb_sub{display:block;}

